<header class="l-footer page-section">
	{% block footer %}
		<footer>
			<div class="container">
				<div class="row">
					{% for footerblock in options.footer_block %}
					    <div class="col-md-4">
					    	<h5>{{footerblock.title}}</h5>
							{{footerblock.content}}
					    </div>
					{% endfor %}

					<div class="col-md-4">
						<h5>Stay up to date</h5>
						{% filter shortcodes %}
							[gravityform id="1" ajax="true" title="false" description="false"]
						{% endfilter  %}
					</div>
				</div>
			</div>
		</footer>
	{% endblock %}
</header>